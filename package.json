{
  "name": "ringcentral-web-phone",
  "version": "0.7.0",
  "scripts": {
    "test": "npm run test:watch -- --single-run",
    "test:watch": "karma start",
    "test:coverage": "cat .coverage/*/lcov.info | coveralls -v",
    "build": "npm run build:webpack",
    "watch": "npm-run-all --print-label --parallel 'build:** -- --watch'",
    "build:webpack": "webpack --config webpack.config.js --progress",
    "build:tsc": "tsc",
    "start": "http-server --port ${PORT:-8080}",
    "prepublishOnly": "npm run build",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint --fix ",
    "lint:all": "npm run lint 'src/**/*.js' 'demo/**/*.js'",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "getstats": "^1.2.0",
    "sip.js": "0.13.5"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.9",
    "@types/karma": "3.0.2",
    "bootstrap": "3.4.0",
    "coveralls": "3.0.2",
    "dotenv": "6.2.0",
    "eslint": "5.14.1",
    "eslint-config-ringcentral-typescript": "0.1.0",
    "http-server": "0.11.1",
    "husky": "1.3.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.3.0",
    "jquery": "3.3.1",
    "karma": "4.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-env": "0.1.0",
    "karma-jasmine": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "lint-staged": "8.1.4",
    "npm-run-all": "4.1.5",
    "ringcentral": "3.2.1",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3"
  },
  "preferGlobal": false,
  "private": false,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {
      "name": "Kirill Konshin"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/ringcentral-web-phone.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-web-phone/issues"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-web-phone",
  "engines": {
    "node": ">=0.10.36"
  },
  "license": "MIT"
}
